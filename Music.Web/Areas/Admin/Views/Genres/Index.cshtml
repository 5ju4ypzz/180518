@model IEnumerable<Music.Model.EF.genre>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}
<a href="~/Admin/Genres/Create">

    <i class="fa fa-fw fa-plus">
    </i>Thêm Ca sĩ
</a>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.genre_name)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.genre_name)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.genre_id }) |
                @Html.ActionLink("Details", "Details", new { id = item.genre_id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.genre_id })
            </td>
        </tr>
    }
</table>