@model IEnumerable<Music.Model.EF.user>

@{
    ViewBag.Title = "Quản lý người dùng";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

<div class="table-heading">
    <h3 class="text-success text-uppercase">Danh sách người dùng</h3>
</div>
<!-- /.panel-heading -->
<div class="agile-tables">
    <table id="tables">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.email)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.user_name)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.user_dateofbirth)
                </th>
                <th width="65px">
                    @Html.DisplayNameFor(model => model.user_sex)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.user_address)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.user_phone)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.user_image)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.status)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.user_createdate)
                </th>
                <th>
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.email)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.user_name)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.user_dateofbirth)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.user_sex)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.user_address)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.user_phone)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.user_image)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.status)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.user_createdate)
                    </td>
                    <td>
                        @Html.ActionLink("Sửa", "Edit", new { id = item.user_id }) |
                        @Html.ActionLink("Chi tiết", "Details", new { id = item.user_id }) |
                        @Html.ActionLink("Xóa", "Delete", new { id = item.user_id })
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>